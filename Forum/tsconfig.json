{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "strict": true,
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./",
      "paths": {
        "@/*": ["resources/js/*"]
      }
    },
    "include": ["resources/js"]
  }
  